Sequence Sequence() #Sequence :
{
    String serverName = null, databaseName = null, schemaName = null, sequenceName = null;
}
{
    (
          LOOKAHEAD(7) serverName=RelObjectName() "." [databaseName=RelObjectName()] "." [sequenceName=RelObjectName()] "." sequenceName=RelObjectName()
        | LOOKAHEAD(5) databaseName=RelObjectName() "." [schemaName=RelObjectName()] "." sequenceName=RelObjectName()
        | LOOKAHEAD(3) schemaName=RelObjectName() "." sequenceName=RelObjectName()
        | sequenceName=RelObjectName()
    )
    {
        final Server server = new Server(serverName);
        final Database database = new Database(server, databaseName);
        Sequence sequence = new Sequence(database, schemaName, sequenceName);
        return sequence;
    }
}

