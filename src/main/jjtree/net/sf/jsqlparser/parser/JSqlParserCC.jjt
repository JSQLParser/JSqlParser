}
{
    <K_SET>
    (

        [LOOKAHEAD(3) (tk = <K_LOCAL> | tk = <K_SESSION>) {effectParameter = tk.image; } ]
        ( LOOKAHEAD(2)
          <K_DATETIMELITERAL> <K_ZONE> { name = "Time Zone"; useEqual=false; }
