        |
        LOOKAHEAD(2)
        stm = AlterTable()
        |
        stm = Merge()
        |
        LOOKAHEAD(CreateIndex())
        stm = CreateIndex()
        |
        LOOKAHEAD(CreateTable())
        stm = CreateTable()
        |
        LOOKAHEAD(CreateView())
        stm = CreateView()
        |
        stm = AlterView()
        |
        stm = Drop()
        |
        stm = Truncate()
        |
        stm = Execute()
        |
        stm = Set()
        |
        stm = Use()
        |
        stm = Commit()
        |
        LOOKAHEAD(CreateSequence())
